docker run --name mysql-gorm -p 3306:3306 -e MYSQL_ROOT_PASSWORD=notsosecretpassword -d mysql

docker exec -it [container_name] bash

mysql -uroot -p

show databases;

create database gorm;

use gorm;

show tables;

SELECT * FROM users;

MYSQL_DB_USER=root MYSQL_DB_PASSWORD=notsosecretpassword MYSQL_DB_HOST=localhost MYSQL_DB_PORT=3306 MYSQL_DB_DATABASE=gorm go run main.go
